<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shareku.mapper.MobileMapper">

	<resultMap type="Mobile" id="mobileResultMap">
		<id column="id" property="id"/>
		<result column="content" property="content"/>
	</resultMap>
	<insert id="insertMobile" parameterType="Mobile"  useGeneratedKeys="true" keyProperty="id">
		insert into mobile (content) values (#content})
	</insert>
</mapper>